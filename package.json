{
  "name": "subgraphs",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080",
  "devDependencies": {
    "concurrently": "^3.5.1",
    "npm-run-all": "^4.1.3"
  },
  "scripts": {
    "start": "concurrently \"npm run prod\" \"npm run frontend\"",
    "sync": "rsync -a frontend/build/ backend/www/ && rm -rf frontend/build",
    "build": "npm run build --prefix frontend",
    "frontend": "npm run start --prefix frontend",
    "backend": "npm run watch --prefix backend",
    "deployBackend": "npm run deploy --prefix backend",
    "deploy": "npm-run-all build sync deployBackend"
  }
}
