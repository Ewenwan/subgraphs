{
    "cSpell.words": [
        "nbsp",
        "quadtree",
        "studio",
        "tensor",
        "subgraphs"
    ],
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "frontend/src/**/*.css": true
    },
    "python.pythonPath": "python2",
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "python.linting.pylintArgs": [
        "--disable=C0111"
    ],
    "python.linting.lintOnSave": true
}